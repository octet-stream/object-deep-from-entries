{
  "type": "module",
  "name": "object-deep-from-entries",
  "version": "0.4.0",
  "description": "Make an object or collection from entries deeply.",
  "main": "./lib/object-deep-from-entries.js",
  "types": "./lib/object-deep-from-entries.ts",
  "exports": {
    "import": {
      "types": "./lib/object-deep-from-entries.ts",
      "default": "./lib/object-deep-from-entries.js"
    },
    "require": {
      "types": "./lib/object-deep-from-entries.cts",
      "default": "./lib/object-deep-from-entries.cjs"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octet-stream/object-deep-from-entries.git"
  },
  "author": "Nick K. <nick.kruchinin@gmail.com>",
  "license": "MIT",
  "keywords": [
    "object",
    "deep",
    "entries",
    "from-entries",
    "object-from-entries",
    "convert",
    "collection"
  ],
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "del-cli lib && tsup",
    "test": "NODE_OPTIONS=\"--no-warnings --import tsx\" ava",
    "eslint": "eslint --fix **/*.ts",
    "staged": "lint-staged",
    "coverage": "c8 npm test",
    "report:html": "c8 -r=html npm test",
    "ci": "npm run test:dts && c8 npm test && c8 report --reporter=json",
    "prepare": "husky install",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "@octetstream/eslint-config": "8.2.5",
    "@types/node": "^20.11.30",
    "ava": "6.1.2",
    "c8": "9.1.0",
    "del-cli": "^5.1.0",
    "eslint": "8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "15.2.2",
    "tsup": "^8.0.2",
    "tsx": "^4.7.1",
    "typescript": "5.4.3"
  }
}
